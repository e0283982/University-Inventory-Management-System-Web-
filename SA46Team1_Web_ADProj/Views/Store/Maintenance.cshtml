@{
    Layout = "~/Views/Shared/_LayoutStore.cshtml";
}


<script>
    $(function () {
        var pageIndex =@Session["MaintenanceTabIndex"].ToString();

        switch (pageIndex) {
            case 1:
                $("#tabs-min").tabs();
                break;
            case 2:
                $("#tabs-min").tabs({ active: 1 });
                break;
            case 3:
                $("#tabs-min").tabs({ active: 2 });
                break;
            case 4:
                $("#tabs-min").tabs({ active: 3 });
                break;
            default:
                $("#tabs-min").tabs();
                break;
            }
        });        
</script>

<div id="tabs-min">
    <ul>
        <li style="font-size: 10pt;"><a href="/Store/StoreMaintenance/Items">Items</a></li>
        <li style="font-size: 10pt;"><a href="/Store/StoreMaintenance/Categories">Categories</a></li>
        <li style="font-size: 10pt;"><a href="/Store/StoreMaintenance/StoreBin">Bin</a></li>
        <li style="font-size: 10pt;"><a href="/Store/StoreMaintenance/Suppliers">Supplier List</a></li>
    </ul>
</div>

